def primeNumber(a,b):
    if a<2:
        print (a,"is not a prime number")
    else:
        mod = a % b
        if b == 1:
            print (a, "is a prime number")
        elif mod == 0:
            print (a, "is not a prime number")
        else:
            b = b - 1
            primeNumber(a,b)

a = int(input("plz int a number"))
b = a-1
primeNumber(a,b)
       
